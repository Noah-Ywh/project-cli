{
  "name": "<%= name %>",
  "version": "1.0.0",
  "private": true,
  "description": "<%= description %>",
  "scripts": {
    "dev": "nuxi dev",
    "build:test": "cross-env NUXT_ENV_MODE=test nuxi build",
    "build:prod": "cross-env NUXT_ENV_MODE=production nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare && npx only-allow pnpm",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.ts,.vue . --fix"
  },
  "keywords": ["<%= description %>"],
  "author": "Noah Yu",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@spruce-hub/nuxt-fetch": "^1.11.1",
    "@spruce-hub/nuxt-route": "^1.11.1",
    "node-fetch-native": "^1.6.4",
    "nuxt": "^3.11.1",
    "nuxt-primevue": "^0.3.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@noahyu/eslint-config-vue": "^1.2.4",
    "@types/node": "^20.12.4",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sass": "^1.74.1",
    "typescript": "^5.4.3"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
